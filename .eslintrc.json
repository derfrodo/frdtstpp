{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-console":"warn",
        "no-unused-vars": "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "sort-keys": [
            "off"
        ],
        "max-len": [
            "warn",
            {
                "code": 120,
                "ignoreComments": true
            }
        ],
        "one-var": "off"
    }
}